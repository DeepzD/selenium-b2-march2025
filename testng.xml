<suite name=" TestNG Example suite">
    name ="chrome" enabled="false">
    <parameter name = "browser" value = "chrome"/>


    <test name ="chrome">
        <parameter name = "browser" value = "edge"/>
        <classes>
            <class name = "com.dd.selenium.HelloSeleniumTest">
            </class>
            <parameter name = "browser" value = "safari"/>
        </classes>
    </test>

    <test name ="firefox" enabled="true">
        <parameter name = "browser" value = "firefox"/>
        <classes>
            <class name = "com.dd.selenium.HelloSeleniumTest">
            </class>
        </classes>

    </test>
"
    <test name ="chrome-headless" enabled="true">
        <classes>
            <class name = "com.dd.selenium.HelloSeleniumTest">
                <methods>
                    <parameter name = "browser" value = "chrome-headless"/>

                </methods>
            </class>
        </classes>
    </test>
</suite>